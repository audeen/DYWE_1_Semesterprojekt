{% extends "_layout" %}

{% set page_title = 'Projekte' %}
{% set projekte = 'current' %}
{% set start, aktuelles, opensource, mods = '','','','' %}


{% block projekte %}
    
{% for entry in craft.entries.section('projekte').limit(10).all() %}

<div class="artikel_1">
    <a href="{{ entry.url }}">
    <h1>{{ entry.title }}</h1>
    </a>
    <h3>{{ entry.artikelTeaser }}</h3>
        {{ entry.artikelBody|chop(limit=2, unit='p') }}
</div>

{% endfor %}
<br>
{% endblock projekte %}

