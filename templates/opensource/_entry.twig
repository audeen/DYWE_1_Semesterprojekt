{% extends "_layout" %}
{% block opensource %}



<div class="artikel_1">
    <a href="{{ siteUrl }}openSourceOverview">
        <h2>{{ entry.section }}</h2>
    </a>

    <a href="{{ entry.url }}">
        <h1>{{ entry.title }}</h1>
    </a>

    <h3>{{ entry.artikelTeaser }}</h3>

    {{ entry.artikelBody }}

    {% for image in entry.artikelImage %}
        <img src="{{ image.geturl ('smallThumb') }}">
    {% endfor %}

</div>
<br>

{% endblock opensource %}
